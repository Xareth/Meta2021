           method: 'POST',\n            headers: { 'Content-Type': 'application/json' },\n            dataType: 'json',\n            body: JSON.stringify(form)\n        };\n        const response = await fetch(methods.route, requestOptions);\n        const result = await response.json();\n        methods.setSubMod(result.route);\n    }\n\n\n    // Render form\n    return (<div>\n        <form onSubmit={AfterFormSubmit}>\n            {form.length > 0 ? form.map((data) => { return <Input\n                key={data.id}\n                id={data.id}\n                label={data.label}\n                type={data.type}\n                field={data.field}\n                update={UpdateOnChange}\n            />  }) : \"\"}\n        </form>\n    </div>)\n}\n\n\nfunction Input(props) {\n    return <div>\n        <label>{props.label}</label>\n        <input id={props.id} type={props.type} onChange={props.update} name={props.label} />\n    </div>\n}\n\nexport default RenderForm;",{"ruleId":"60","replacedBy":"61"},{"ruleId":"62","replacedBy":"63"},{"ruleId":"64","severity":1,"message":"65","line":1,"column":17,"nodeType":"66","messageId":"67","endLine":1,"endColumn":26},{"ruleId":"64","severity":1,"message":"68","line":5,"column":29,"nodeType":"66","messageId":"67","endLine":5,"endColumn":46},{"ruleId":"64","severity":1,"message":"69","line":10,"column":14,"nodeType":"66","messageId":"67","endLine":10,"endColumn":26},{"ruleId":"64","severity":1,"message":"70","line":1,"column":16,"nodeType":"66","messageId":"67","endLine":1,"endColumn":24},{"ruleId":"71","severity":1,"message":"72","line":11,"column":12,"nodeType":"73","endLine":11,"endColumn":14,"suggestions":"74"},{"ruleId":"75","severity":1,"message":"76","line":14,"column":37,"nodeType":"77","endLine":14,"endColumn":101},{"ruleId":"71","severity":1,"message":"72","line":8,"column":8,"nodeType":"73","endLine":8,"endColumn":10,"suggestions":"78"},{"ruleId":"64","severity":1,"message":"79","line":1,"column":8,"nodeType":"66",